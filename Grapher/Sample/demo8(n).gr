               line=on line.width=2 line.dash_list=(4,4) line.color=cyan
        type=6 nCopy=0 legend.txt="HF" symbol.size=3 symbol.color=red hollow
               line=on line.width=2 line.dash_list=(4,4) line.color=magenta
   axis	type=x loc=lower GRID_STYLE LABEL_STYLE
	       title.size=10 title="Vg [V]"
        type=x loc=upper label=off
        type=y loc=lower GRID_STYLE LABEL_STYLE
               title.size=10 title="Cg [F/cm^2]" title.spaceFactor=9
        type=y loc=upper label=off
   view|28

