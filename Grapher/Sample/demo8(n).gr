%grBeginning
_defn "view|18" "view x1=0.15 x2=0.48 y1=0.84 y2=1.09"
_defn "view|28" "view x1=0.58 x2=0.92 y1=0.84 y2=1.09"
_defn "view|38" "view x1=0.15 x2=0.48 y1=0.53 y2=0.78"
_defn "view|48" "view x1=0.58 x2=0.92 y1=0.53 y2=0.78"
_defn "view|58" "view x1=0.15 x2=0.48 y1=0.22 y2=0.47"
_defn "view|68" "view x1=0.58 x2=0.92 y1=0.22 y2=0.47"
_defn "view|78" "view x1=0.15 x2=0.48 y1=-.09 y2=0.16"
_defn "view|88" "view x1=0.58 x2=0.92 y1=-.09 y2=0.16"
_defn "GRID_STYLE" "gridmajor=on gridmajor.color=green2 \
                   \gridminor=on gridminor.color=green1 \
                   \gridminor.dash_list=shortdashed"

_defn "LABEL_STYLE" "label.org=tick label.size=9 label.align=22"

.glob 
   gtxt txt="DEMO-8(N) / IDEAL NMOS CAPACITOR (SiO2)"
        t.size=20 txt.spaceFactor=1
   gout portrait

.plot
   attr xlin ylin zlin
   data type=user
   xyzb xlb=-3 xub=3 ylb=0 yub=5e-6
   slct ____ xno=6 yno=11,14
        type=1 nCopy=0 legend.txt="LF" symbol.size=3 symbol.color=blue hollow
               line=on line.width=2 line.dash_list=(4,4) line.color=cyan
        type=6 nCopy=0 legend.txt="HF" symbol.size=3 symbol.color=red hollow
               line=on line.width=2 line.dash_list=(4,4) line.color=magenta
   ufun type=IdealMosCapacitor
               option=2 suboption=1 sweep=-2.5,2.5,0.1 argument=n,290,4.1,0,10,3.9,7.035e18
        type=MatrixPrint argument=demo8(n).gr.dat
   axis	type=x loc=lower GRID_STYLE LABEL_STYLE
	       title.size=10 title="Vg [V]"
        type=x loc=upper label=off
        type=y loc=lower GRID_STYLE LABEL_STYLE
               title.size=10 title="Cg [F/cm^2]" title.spaceFactor=9
        type=y loc=upper label=off
   view|18

.plot
   attr xlin ylog zlin
   data type=prev
   xyzb xlb=-3 xub=3 ylb=1e-7 yub=1e-5
